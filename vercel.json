{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "installCommand": "python -m pip install -r requirements.txt",
    "devCommand": "python -m api.main",
    "rewrites": [
        {
            "source": "/(.*)*",
            "destination": "/api/main.py"
        }
    ],
    "fluid": true,
    "framework": "fastapi"
}
